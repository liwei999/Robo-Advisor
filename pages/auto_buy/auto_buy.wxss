/* pages/auto_buy/auto_buy.wxss */
page{background-color: #F0F6F9; display: flex; flex-direction: column;}
.one{display:flex;flex-direction: column;  background-color: #ffffff;}
.one_item{display:flex;flex-direction: row;border-top: 1px solid #E1E6E9;}
.one_item view:first-child{color: #888888; padding: 40rpx 58rpx 42rpx 32rpx; font-size: 30rpx;}
.one_item view:last-child{color: #000000;padding-top: 43rpx; font-size: 30rpx;}

.one_three{display:flex;flex-direction: row;border-top: 1px solid #E1E6E9; position:relative;}
.one_three view:nth-of-type(1){color: #888888; padding: 40rpx 58rpx 42rpx 32rpx; font-size: 30rpx;}
.one_three view view:nth-of-type(1){color: #000000;padding: 0rpx; padding-top:20rpx;}
.one_three view view:nth-of-type(2){color: #000000;padding-top:20rpx;font-size: 24rpx;}
.three_reight{ color: #CCCCCC; font-size: 41rpx; position: absolute; right: 30rpx; top:50%;margin-top: -20.5rpx;}
.two{ margin: 24rpx 0 100rpx 0; height: 600rpx;}
.btn{position: fixed;padding: 33rpx 0;font-size: 32rpx; background-color: #888888;color: #ffffff; width: 100%; bottom: 0rpx; text-align: center;}
