/* pages/third/third.wxss */
page{background-color: #F0F6F9; display: flex; flex-direction: column;}
.count{ margin: 40rpx 40rpx 0rpx 40rpx;  background-color: #ffffff; border-radius: 20rpx; padding: 0px 24px;}

.second{ margin-top: 24rpx; }

.third{ margin-bottom: 14rpx;}
.third_char{background-color: #F6FCFF; border: 1px solid #E5EDF2;  height: 416rpx; margin-bottom: 24rpx;}
.third_char image{width: 100%; height: 100%;}

.count_title{font-size: 26rpx; color:#888888; margin: 23rpx 0 20rpx 0;}
.first_char{ background-color: #F6FCFF; border: 1px solid #E5EDF2;  height: 340rpx; margin-bottom: 24rpx;}
.first_char image{ width: 100%;height: 100%;}
.first_table{border: 1px solid #83C7F0;margin-bottom:20rpx}
.first_table .first_row{display: flex;flex-direction: row; text-align: center;}
.first_row_count{padding-bottom: 22rpx; padding-top: 21rpx;border-bottom: 1px solid #D8E2E8; background-color: #F6FCFF;}
.first_row_count1{flex: 2; font-size: 28rpx; border-right:1px solid #D8E2E8;}
.first_row_count2{flex: 3;font-size: 30rpx;}
.first_jy{ padding: 15rpx; font-size: 26rpx;color:#83C7F0}

.font_red{color: red;}
.font_green{color: green;}
.remarks{font-size: 24rpx;color:#717A7D;margin: 16rpx 0 29rpx 0rpx;}

.row{display: flex;flex-direction:row; align-items: center; height: 70rpx; overflow: hidden; text-align: center;border-bottom: 1px solid #D8E2E8;}
.row:nth-child(odd){background-color: #F6FCFF;}
.row:nth-child(even){background-color: #ffffff;}
.row:last-child{border-bottom: 0px;}
.row view{ height: 100%;line-height: 70rpx;}
.row view:first-child{flex: 3;font-size: 28rpx;color:#000000;border-right:1px solid #D8E2E8;}
.row view:last-child{flex: 2;font-size:30rpx;}
.four{display: flex; flex-direction:row; margin-bottom:44rpx;}
.four button{ width: 300rpx;height: 90rpx; border: #19A7E9 solid 1rpx; font-size: 32rpx; color: #19A7E9; border-radius: 45rpx; margin-top: 16rpx;}
/**
下边列表 开始
*/

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.drawer_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  height: 880rpx;
}

.menu_title{font-size: 36rpx;color: #000000;padding-top: 27rpx; padding-bottom: 27rpx; text-align: center; border: solid 1px #d9d9d9; display:flex;
align-items:center;
justify-content:space-between;
}
.menu_title cover-view{flex: 9;}
.menu_title cover-image{position: absolute; right: 30rpx;}
/*银行卡 开始  */
.item{border-bottom: 1rpx solid #E1E6E9;color: #000000; margin-left: 20rpx;padding: 46rpx 0; text-align: center;font-size: 72rpx;}
.item_bank{margin-left: 20rpx; display: flex;flex-direction: row; height: 120rpx;justify-content:space-between; align-items: center;}
.line{height: 1rpx;background-color: #E1E6E9;margin-left: 20rpx;}
.bank_ok{ width: 30rpx; height: 30rpx; flex: 1;}
.bank_icon{width: 50rpx; height: 50rpx; flex: 1;}  
.bank_icon cover-image{width: 50rpx;height: 50rpx;}
.bank_ok cover-image{width: 30rpx; height: 30rpx;}
.bank_text{font-size: 32rpx; flex:8;}
.add_text{flex: 9;font-size: 32rpx;}
/*银行卡 结束  */

/* 支付提醒 开始*/
.item_Prompt{color: #000000; margin-left: 20rpx;padding: 67rpx 152rpx; font-size: 24rpx; display: flex;justify-content: center;white-space: pre-line;}
.prompt{display: flex;flex-direction:row;justify-content:space-around; margin-bottom: 60rpx;}
.prompt cover-view{ width: 280rpx;height:80rpx; font-size: 32rpx;border-radius: 10rpx; border: #888888 1rpx solid; text-align: center; line-height: 80rpx;}
.btn_l{background-color: #fff;border: #888888; color: #888888}
.btn_r{background-color: #888888;color: #FFFFFF;}
/* 支付提醒 结束*/


